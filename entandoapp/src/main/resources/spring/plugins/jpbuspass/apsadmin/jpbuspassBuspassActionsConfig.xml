<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:context="http://www.springframework.org/schema/context"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
				http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd" >

	<bean id="jpbuspassBuspassPluginSubMenu" class="com.agiletec.apsadmin.system.plugin.PluginSubMenuContainer" >
		<property name="subMenuFilePath"><value>/WEB-INF/plugins/jpbuspass/apsadmin/jsp/common/layouts/subMenu.jsp</value></property>
	</bean>

	<bean id="jpbuspassBuspassFinderAction" scope="prototype" 
			class="org.entando.entando.plugins.jpbuspass.apsadmin.buspass.BuspassFinderAction" 
			parent="abstractBaseAction" >
		<property name="buspassManager" ref="jpbuspassBuspassManager" />
	</bean>

	<bean id="jpbuspassBuspassAction" scope="prototype" 
			class="org.entando.entando.plugins.jpbuspass.apsadmin.buspass.BuspassAction" 
			parent="abstractBaseAction" >
		<property name="buspassManager" ref="jpbuspassBuspassManager" />
	</bean>


	<bean id="jpbuspassBuspassConfigAction" class="org.entando.entando.plugins.jpbuspass.apsadmin.portal.specialwidget.buspass.BuspassConfigAction"
		parent="simpleWidgetAction" scope="prototype">
		<property name="buspassManager" ref="jpbuspassBuspassManager" />
	</bean>


	<!-- INTERNAL_SERVLET -->
	<bean id="jpbuspassBuspassFinderFrontEndAction" scope="prototype" 
			class="org.entando.entando.plugins.jpbuspass.aps.internalservlet.buspass.BuspassFinderFrontEndAction" 
			parent="abstractBaseAction" >
		<property name="buspassManager" ref="jpbuspassBuspassManager" />
	</bean>
	
	<bean id="jpbuspassBuspassFrontEndAction" scope="prototype" 
			class="org.entando.entando.plugins.jpbuspass.aps.internalservlet.buspass.BuspassFrontEndAction" 
			parent="abstractBaseAction" >
		<property name="buspassManager" ref="jpbuspassBuspassManager" />
	</bean>
 	
 	
</beans>